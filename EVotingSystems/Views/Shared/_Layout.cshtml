<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/CastVote/css/reset.css" rel="stylesheet" />
    <script src="~/Content/CastVote/js/prefixfree.min.js"></script>
    <link href="~/Content/CastVote/css/mystyle.css" rel="stylesheet" />
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
  
    <div class="container body-content">
        @RenderBody()
       
        <footer>
            
            <div class="copyright">
                <p>Copyright &copy; 2016. Created by <a href="http://iVote.com" target="_blank">iVote Pakistan</a></p>
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
