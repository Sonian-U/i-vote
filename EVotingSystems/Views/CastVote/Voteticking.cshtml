
@{
    ViewBag.Title = "Voteticking";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Cast Now</title>
    <script src="http://s.codepen.io/assets/libs/modernizr.js" type="text/javascript"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

  
    <link href="~/Content/CastVote/Votetick/css/normalize.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/CastVote/Votetick/css/style.css">



</head>
<body>
    <h1 style="text-align:center"> Vote Now </h1>
    @using (Html.BeginForm("SubmitVote", "CastVote", FormMethod.Post))
    {

    <table class="responsive-table responsive-table-input-matrix">
        <tr>
            <th>Party</th>
            <th>MNA</th>
            <th>MPA</th>
            <th>Intekhabi Nishan </th>
           
        </tr>
        <tr>
            <td data-th="Role">Party 1</td>
            <td data-th="Add to Page"><input type="checkbox" /></td>
            <td data-th="Configure"><input type="checkbox" /></td>
           
            
        </tr>
        <tr>
            <td data-th="Role">Party 2</td>
            <td data-th="Add to Page"><input type="checkbox" /></td>
            <td data-th="Configure"><input type="checkbox" /></td>
          
           
        </tr>
        <tr>
            <td data-th="Role">Party 3</td>
            <td data-th="Add to Page"><input type="checkbox" /></td>
            <td data-th="Configure"><input type="checkbox" /></td>
           
        
        </tr>

       

    </table>
    <p style="text-align:center"><input type="submit" name="sent" value="Submit"></p>


       <p style="text-align:center"> @ViewBag.message</p>
    }
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src="~/Content/CastVote/Votetick/js/index.js"></script>



</body>
</html>
