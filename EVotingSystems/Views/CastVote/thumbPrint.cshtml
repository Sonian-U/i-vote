
@{
    ViewBag.Title = "thumbPrint";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<body>


    @using (Html.BeginForm("thumbPrint", "CastVote", FormMethod.Post))
    { <div class="login">
        <h1>Verification</h1>
        <div class="form">


            <p class="message">
                <img class="img-responsive" style="margin:0 auto;" src="~/Content/CastVote/thumb.jpg" alt="" />
            </p>

            <p class="submit"><input type="submit" name="sent" value="Verify"></p>
            <p class="message">@Html.ActionLink("Go Back", "MobileVerification", "CastVote")</p>
        </div>

    </div>
    
    }
</body>

